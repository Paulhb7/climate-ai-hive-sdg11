# Configuration Groq (requis)
# Obtenez votre clé API sur https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# Configuration WatsonX (optionnel)
# Obtenez vos identifiants sur https://dataplatform.cloud.ibm.com/
WATSONX_PROJECT_ID=your_watsonx_project_id_here
WATSONX_API_KEY=your_watsonx_api_key_here
WATSONX_API_URL=https://us-south.ml.cloud.ibm.com  # optionnel, URL par défaut

# Configuration des modèles par défaut (optionnel)
# Provider par défaut (groq ou watsonx)
DEFAULT_PROVIDER=groq

# Modèles par défaut pour chaque provider
DEFAULT_GROQ_MODEL=qwen/qwen3-32b
DEFAULT_WATSONX_MODEL=llama-3-3-70b-instruct

# Modèle spécifique pour l'agent des projets UN (optionnel)
# Si non défini, utilise le modèle par défaut du provider
UN_PROJECTS_MODEL=llama-3.1-8b-instant 